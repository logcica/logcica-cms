{
  "name": "logcica-cms",
  "description": "logCiCA CMS",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@nouance/payload-better-fields-plugin": "^1.4.1",
    "@lexical/clipboard": "0.13.1",
    "@lexical/code": "0.13.1",
    "@lexical/file": "0.13.1",
    "@lexical/hashtag": "0.13.1",
    "@lexical/headless": "0.13.1",
    "@lexical/html": "0.13.1",
    "@lexical/link": "0.13.1",
    "@lexical/list": "0.13.1",
    "@lexical/mark": "0.13.1",
    "@lexical/markdown": "0.13.1",
    "@lexical/overflow": "0.13.1",
    "@lexical/plain-text": "0.13.1",
    "@lexical/react": "0.13.1",
    "@lexical/rich-text": "0.13.1",
    "@lexical/selection": "0.13.1",
    "@lexical/table": "0.13.1",
    "@lexical/utils": "0.13.1",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.7.3",
    "@payloadcms/live-preview-react": "^0.2.0",
    "@payloadcms/plugin-cloud": "^3.0.2",
    "@payloadcms/plugin-form-builder": "^1.2.2",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-redirects": "^1.0.2",
    "@payloadcms/plugin-seo": "^2.3.2",
    "@payloadcms/richtext-lexical": "^0.11.4",
    "@swc/core": "^1.7.39",
    "@types/react-dom": "^18.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.5",
    "eslint-config-next": "^15.0.1",
    "express": "^4.21.1",
    "lexical": "0.13.1",
    "next": "^15.0.1",
    "node-cron": "^3.0.3",
    "payload": "^2.30.3",
    "payload-admin-bar": "^1.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.1",
    "@payloadcms/eslint-config": "1.1.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "copyfiles": "^2.4.1",
    "nodemon": "3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  }
}
